{
  "version": "2.0",

  "extensions": {
    "durableTask": {
      "hubName": "perftest2",
      "UseGracefulShutdown": true,
      "storageProvider": {
        "type": "Netherite",
        "StorageConnectionName": "AzureWebJobsStorage",
        "EventHubsConnectionName": "EventHubsConnection"
      }
    }
  },

  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "SamplePerformances": "Debug"
    }
  }
}